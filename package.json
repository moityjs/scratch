{
  "name": "scratch",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "2.2.5",
    "mdi-react": "^2.1.19",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-router-dom": "^4.2.2",
    "react-toolbox": "^2.0.0-beta.12",
    "react-transition-group": "1.1.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "coverage": "react-app-rewired test --env=jsdom --coverage",
    "stylelint": "stylelint src/**/*.css",
    "eslint": "eslint src",
    "lintfix": "npm run -s stylelint -- --fix & npm run -s eslint -- --fix || true",
    "precommit": "lint-staged",
    "yarn_if_lock_changed": "git diff-tree -r --name-only --no-commit-id \"@{1}\" \"@\" | grep --quiet yarn.lock && yarn || true",
    "postcheckout": "npm run -s yarn_if_lock_changed",
    "postmerge": "npm run -s yarn_if_lock_changed",
    "postrewrite": "npm run -s yarn_if_lock_changed"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "caniuse-lite": "1.0.30000810",
    "eslint": "^4.18.1",
    "eslint-config-majestic": "^7.4.1",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-more": "^0.3.1",
    "eslint-plugin-mysticatea": "^4.2.4",
    "eslint-plugin-optimize-regex": "^1.1.5",
    "eslint-plugin-prefer-class-properties": "^1.0.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-sort-class-members": "^1.3.0",
    "eslint-plugin-unicorn": "^4.0.2",
    "eslint-plugin-variables": "^1.0.3",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.3.1",
    "gitmoji-cli": "^1.8.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-import": "^11.1.0",
    "postcss-url": "^7.3.1",
    "react-app-rewire-hot-loader": "^1.0.0",
    "react-app-rewired": "^1.4.1",
    "react-scripts": "^1.1.1",
    "stylelint": "^9.1.1"
  },
  "resolutions": {
    "eslint": "^4.7.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-config-react-app": "./empty-module"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/.eslintrc.js",
      "!src/registerServiceWorker.js",
      "!src/.eslintrc.js",
      "!src/index.js"
    ]
  }
}
